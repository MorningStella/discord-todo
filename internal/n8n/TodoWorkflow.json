      "parameters": {
        "jsCode": "let text = $('Webhook').first().json.body.text;\nlet query = null;\nlet channel_id = $input.first().json.channel_id;\nlet tag = $input.first().json.tag;\n// Check if text contains \"all\"\nif (text && text?.includes(\"all\") && tag) { \n  query = `{\"$and\":[{\"channel_id\": \"${channel_id}\"},{\"tag\":\"${tag}\"}]}`\n} else if (tag) // Âè™Êúâ tag\n{\n  query = `{\"$and\":[{\"channel_id\": \"${channel_id}\"},{\"done\":\":x:\"},{\"tag\":\"${tag}\"}]}`\n} else if (text?.includes(\"all\"))\n{\n    query = `{\"$and\":[{\"channel_id\": \"${channel_id}\"}]}`\n} else {\n  query = `{\"$and\":[{\"channel_id\": \"${channel_id}\"},{\"done\":\":x:\"}]}`\n}\n\n// Return the query object\nreturn {\n  query: query\n};"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [140, 120],
